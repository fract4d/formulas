Icon_3{
z1=pixel, z=0
l=real(p1),a=imag(p1)
b=real(p2),g=imag(p2):
zm=z1*z1
fac=l+a*|z1|+b*real(z1*zm)
z1=fac*z1+g*conj(zm)
z=z+(z1-pixel)
|z1|<=200
}

